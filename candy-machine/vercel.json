{
  "builds": [
    {
      "src": "mint_ui/package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "pnpm install",
        "buildCommand": "pnpm run build && pnpm run start"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mint_ui/$1"
    }
  ]
}
