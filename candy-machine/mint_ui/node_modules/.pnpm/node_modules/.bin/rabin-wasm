#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/cli/node_modules:/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/node_modules:/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules:/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/cli/node_modules:/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules/rabin-wasm/node_modules:/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/rabin-wasm@0.1.5/node_modules:/home/aurelien/Alyra-Solana/candy-machine/mint_ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rabin-wasm/cli/bin.js" "$@"
else
  exec node  "$basedir/../rabin-wasm/cli/bin.js" "$@"
fi
